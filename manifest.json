{
    "manifest_version": 3,
    "name": "Waterloo Extension for Change",
    "version": "1.0.0",
    "permissions": ["tabs", "storage"],
    
    "action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://quest.pecs.uwaterloo.ca/*"],
            "css": ["css/quest.css"],
            "run_at": "document_end"
        }
    ],
    "options_page": "html/options.html"
}